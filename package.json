{
  "name": "pmc",
  "version": "1.0.0",
  "description": "PMC Core is a powerful Command Line Interface tool designed for developers to efficiently manage and organize their project workspaces. As the next evolution of the original PMC, it offers a refined structure, improved performance, and new features to supercharge your development workflow.",
  "main": "temp/pmc.js",
  "bin": "temp/pmc.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildtsc": "tsc",
    "dev": "mkdir temp && tsc && node dist/pmc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IzanamiiDevv/PMC-Core.git"
  },
  "pkg": {
    "assets": [
      "node_modules/inquirer/**/*"
    ],
    "scripts": "temp/**/*.js",
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "bin"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IzanamiiDevv/PMC-Core/issues"
  },
  "homepage": "https://github.com/IzanamiiDevv/PMC-Core#readme",
  "dependencies": {
    "inquirer": "^8.2.6",
    "nanospinner": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
